<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<configSections>
		<section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
	</configSections>
	
	<appSettings>
		<add key="webpages:Version" value="3.0.0.0" />
		<add key="webpages:Enabled" value="false" />
		<add key="ClientValidationEnabled" value="true" />
		<add key="UnobtrusiveJavaScriptEnabled" value="true" />
	
		<!--
			In order to use this sample outside of localhost, you will need a license for the Web PKI plugin
			for the domain on which you'll host you web application. Once you have a license, set it below
			(use the "binary version" of the license, not the JSON version).
			
			If you are running this project on localhost, ignore this setting.
		-->
		<add key="WebPkiLicense" value="" />
	
	</appSettings>

	<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
		<targets async="true">
			<target name="log" xsi:type="File" fileName="${specialfolder:folder=ApplicationData}/PkiSdkMvcSamples.log" layout="${longdate} ${level}: ${message}${onexception:${newline}${exception:format=tostring}}" />
		</targets>
		<rules>
			<logger name="*" writeTo="log" minlevel="Info" />
		</rules>
	</nlog>
	
	<system.web>
		<authentication mode="None" />
		<compilation debug="true" targetFramework="4.5.2" />
		<httpRuntime targetFramework="4.5.2" />
		<httpModules>
		</httpModules>
	</system.web>
	
	<system.webServer>
		<modules>
			<remove name="FormsAuthentication" />
		</modules>
		<validation validateIntegratedModeConfiguration="false" />
	<handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers></system.webServer>
	
	<runtime>
